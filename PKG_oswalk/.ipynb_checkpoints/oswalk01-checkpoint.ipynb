    "# %load 10-1-5-使用os.walk列出所有Python檔案.py\n",
    "import os\n",
    "path = \"./data\"\n",
    "for root, dirs, files in os.walk(path):\n",
    "    for file in files:\n",
    "        if file.endswith(\".py\"):\n",
    "             print(os.path.join(root, file))"
